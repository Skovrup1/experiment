Vec2 :: struct {
    x: I32,
    y: I32,
}

make_vec2 :: (x: I32, y: I32) -> Vec2 {
    return {1, 2};
}

main :: () -> I32 {
    a := Vec2{1, 2};
    return a.x;
}

